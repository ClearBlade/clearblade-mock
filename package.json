{
  "name": "@clearblade/clearblade-mock",
  "version": "0.0.1",
  "description": "Utility library for mocking ClearBlade native libraries in unit tests",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig-build.json",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClearBlade/clearblade-mock.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ClearBlade/clearblade-mock/issues"
  },
  "homepage": "https://github.com/ClearBlade/clearblade-mock#readme",
  "dependencies": {
    "@clearblade/code-service-types": "^3.9.7",
    "@types/jest": "^29.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.2",
    "@swc/jest": "^0.2.22",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "typescript": "^4.8.4"
  }
}
